{
  "rules": {
    "users": {
      "$uid": {
        ".read": "$uid === auth.uid",
        ".write": "$uid === auth.uid",
        "activities": {
          ".indexOn": ["timestamp"],
          "$activityId": {
            ".validate": "newData.hasChildren(['type', 'timestamp', 'userId'])"
          }
        }
      }
    }
  }
}

